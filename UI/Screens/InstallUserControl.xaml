<Window x:Class="GUI.InstallUserControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="InstallPlugin" Height="290" Width="422" >
    <Grid>
        <Image Height="75" HorizontalAlignment="Center" Margin="12,12,318,0" Name="pluginImage"  Stretch="Fill" VerticalAlignment="Top" Width="75" />
        <Label Content="Install Name Version?" Height="28" HorizontalAlignment="Left" Margin="94,12,0,0" Name="lblPlugin" VerticalAlignment="Top" Width="357" />
        <Label Content="Description" Height="90" HorizontalAlignment="Left"  Name="lblDescription" VerticalAlignment="Top" Width="378" Margin="10,93,0,0" />
        <Label Content="by Author" Height="29" HorizontalAlignment="Left" Margin="94,31,0,0" Name="lblAuthor" VerticalAlignment="Top" Width="191" />
        <Button Content="Install" Height="21"  Margin="235,66,94,163" Name="btnInstall" Width="71" Click="btnInstall_Click" />
        <Button Content="Cancel" Height="21" Margin="312,66,17,163" Name="btnCancel" Width="71" Click="btnCancel_Click" />
    </Grid>
</Window>
